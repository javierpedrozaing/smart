<p id="notice"><%= notice %></p>

<h1>Listing Grupo Itemes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grupo_itemes.each do |grupo_item| %>
      <tr>
        <td><%= link_to 'Show', grupo_item %></td>
        <td><%= link_to 'Edit', edit_grupo_item_path(grupo_item) %></td>
        <td><%= link_to 'Destroy', grupo_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grupo item', new_grupo_item_path %>
