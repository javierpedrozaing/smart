<p id="notice"><%= notice %></p>

<h1>Listing Secretarias Educaciones</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @secretarias_educaciones.each do |secretarias_educacion| %>
      <tr>
        <td><%= secretarias_educacion.descripcion %></td>
        <td><%= link_to 'Show', secretarias_educacion %></td>
        <td><%= link_to 'Edit', edit_secretarias_educacion_path(secretarias_educacion) %></td>
        <td><%= link_to 'Destroy', secretarias_educacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Secretarias educacion', new_secretarias_educacion_path %>
