<p id="notice"><%= notice %></p>

<h1>Listing Evaluacion Respuestas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluacion_respuestas.each do |evaluacion_respuesta| %>
      <tr>
        <td><%= link_to 'Show', evaluacion_respuesta %></td>
        <td><%= link_to 'Edit', edit_evaluacion_respuesta_path(evaluacion_respuesta) %></td>
        <td><%= link_to 'Destroy', evaluacion_respuesta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

