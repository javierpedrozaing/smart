    <% end %>
    <!-- Cargamos el parcial modal de videotutorial-->
    <% if defined?(current_persona) %>
    <%= render "layouts/tutoriales/modalvideotutorial" %>
    <% end %>
    <!-- Cargamos el parcial modal de pdf-->
    <%= render "layouts/tutoriales/modalpdf" %>
    <%= render "layouts/tutoriales/modalpdfGrabacion" %>
    <%= render "layouts/tutoriales/modalpdf_manualuso" %>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.9/js/jquery.dataTables.min.js"></script>
     <script type="text/javascript" src="https://cdn.datatables.net/1.10.9/js/dataTables.bootstrap.min.js"></script>
     <script type="text/javascript" src="https://cdn.datatables.net/responsive/1.0.7/js/dataTables.responsive.min.js"></script>
     
  </body>
</html>
